        <xsd:annotation>
          <xsd:documentation>PaymentType, also referred to as card Type - Charge, revolve, Loan etc
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="acctPaymentTypeDS" type="domain:EnumPaymentTypeDSType">
        <xsd:annotation>
          <xsd:documentation>PaymentType description
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
